
@{
    Layout = null;
    int count = 1;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Register</title>
</head>
<body>
    <div> 
        <ul>
            @for (int i = 0; i < 100; i++)
            {
                <li>@count</li>
                count++;
            }
        </ul>
    </div>
</body>
</html>
